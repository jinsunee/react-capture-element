{
  "name": "react-capture-element",
  "version": "0.1.21",
  "author": "Jinsun Park",
  "repository": {
    "type": "git",
    "url": "https://github.com/jinsunee/react-capture-element.git"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.modern.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.modern.js",
      "import": "./dist/index.modern.mjs",
      "default": "./dist/index.umd.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "rm -rf dist && microbundle build -f modern,umd --globals react=React,react/jsx-runtime=jsxRuntime,@emotion/styled=styled --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react",
    "postcompile": "cp dist/index.modern.mjs dist/index.modern.js && cp dist/index.modern.mjs.map dist/index.modern.js.map",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "license": "MIT",
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^6.5.13",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "html2canvas": "^1.4.1"
  }
}
